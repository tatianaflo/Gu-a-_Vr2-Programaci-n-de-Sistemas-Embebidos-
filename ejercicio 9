int ledPins[] = {2,3,4,5,6,7,8};
void setup() {
for (int i = 0; i <7; i++) pinMode(ledPins [i], OUTPUT);
}
void loop() {
for(int i = 0; i < 7; i++){
  digitalWrite(ledPins [i],HIGH);
  delay(200);
  digitalWrite(ledPins [i],LOW);
}
for ( int i = 5; i >= 0; i--){
  digitalWrite(ledPins[i], HIGH);
  delay (200);
  digitalWrite(ledPins[i],LOW);
}
}
